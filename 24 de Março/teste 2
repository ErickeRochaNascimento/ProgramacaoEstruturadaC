#include <stdio.h>

int main() {
    float imprimeDivisaoReal;
    int num1,num2,imprimeDivisaoInteira;
    
    printf("Calculadora");
    printf("\nDigite seu Calculo: ");
    scanf("%d%d", &num1, &num2);
    
    imprimeDivisaoReal = num1 / num2;
    imprimeDivisaoInteira = num1 / num2;
    
   
    
    divisao = num1 % num2;
    if(divisao == 0){
        printf("%d é divisivel por %d",num1,num2);
