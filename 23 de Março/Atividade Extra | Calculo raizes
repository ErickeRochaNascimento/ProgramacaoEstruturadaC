#include <stdio.h>
#include <math.h>

int main() {
    float a,b,c,delta,x1,x2;
    printf("\nDigite a: ");
    scanf("%f",&a);
    printf("Digite b: ");
    scanf("%f",&b);
    printf("Digite c: ");
    scanf("%f",&c);
    
    delta = a*a-4*a+c;
    
    if(delta >= 0){
        x1 = (-b + sqrt(delta)) / (2*a);
        x2 = (-b - sqrt(delta)) / (2*a);
        printf("Raizes da equacao: %f, %f", x1, x2);
    }
    else
    {
        printf ("Nao existem raizes reais");
    }
        return 0;
    }