#include <stdio.h>

int main() {
   float nota1,nota2,nota3,media;
   
   printf("Calculo nota alunos");
   
   printf("\nDigite a primeira nota: ");
   scanf("%f",&nota1);
   printf("Digite a segunda nota: ");
   scanf("%f",&nota2);
   printf("Digite a terceira nota: ");
   scanf("%f",&nota3);
   
   media = (nota1 + nota2 + nota3)/3;
   
   if(media >= 7){
       printf("Aprovado(a) com média %f",media);
   }else{
       printf("Reprovado(a) com média %f",media);
   }
}