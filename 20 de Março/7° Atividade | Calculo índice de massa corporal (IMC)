#include <stdio.h>

int main() {
    float peso, altura, imc;
    printf("INDICE DE MASSA CORPORAL");
    printf("\nDigite o peso em kg: ");
    scanf("%f",&peso);
    printf("Digite a altura em m: ");
    scanf("%f",&altura);
    imc = (peso/(altura * altura));
    printf("Valor do IMC: %f",imc);

    return 0;
}